{
  "name": "socket.io-nats-adapter",
  "version": "0.0.6",
  "description": "NATS emitter for Socket.io",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "tsc && nyc ts-mocha test/index.ts --exit",
    "format:check": "prettier --check 'lib/**/*.ts' 'test/**/*.ts'",
